{
  "name": "cv",
  "sasslintConfig": "./.sasslintrc.json",
  "scripts": {
    "dev": "webpack-dev-server --config webpack.dev.js",
    "prod": "webpack --config webpack.prod.js",
    "api": "set NODE_ENV=dev&& nodemon api/server.js",
    "prod_api": "set NODE_ENV=prod&& nodemon api/server.js",
    "lint": "eslint src/js/*.jsx",
    "lint-sass": "sass-lint --no-exit --verbose -c .sass-lint.yml",
    "start": "concurrently \"yarn api\" \"yarn dev\""
  },
